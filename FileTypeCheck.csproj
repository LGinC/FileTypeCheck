<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net9.0;net8.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>0.0.1</Version>
    <Authors>LGinC</Authors>
    <PackageId>FileTypeCheck</PackageId>
    <Product>FileTypeCheck</Product>
    <PackageProjectUrl>https://github.com/lginc/FileTypeCheck</PackageProjectUrl>
    <RepositoryUrl>https://github.com/lginc/FileTypeCheck</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>file;FileType;magicBytes</PackageTags>
    <PackageDescription>Check File Type By MagicBytes</PackageDescription>
  </PropertyGroup>

  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>

</Project>
